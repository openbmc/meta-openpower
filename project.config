[project]
	description = Layer for OpenPower machine support
[access]
	inheritFrom = openbmc-all-repos
[submit]
	action = rebase if necessary
[access "refs/heads/*"]
	abandon = group github/openbmc/meta-openpower-maintainers
	create = group github/openbmc/meta-openpower-maintainers
	forgeCommitter = group github/openbmc/meta-openpower-maintainers
	push = +force group github/openbmc/meta-openpower-maintainers
	label-Code-Review = -2..+2 group github/openbmc/meta-openpower-maintainers
	label-Ok-To-Test = +0..+1 group github/openbmc/meta-openpower-maintainers
	label-Verified = -1..+1 group github/openbmc/meta-openpower-maintainers
	rebase = group github/openbmc/meta-openpower-maintainers
	submit = group github/openbmc/meta-openpower-maintainers
	editTopicName = group github/openbmc/meta-openpower-maintainers
[access "refs/tags/*"]
	createTag = group github/openbmc/meta-openpower-maintainers
	createSignedTag = group github/openbmc/meta-openpower-maintainers
	push = +force group github/openbmc/meta-openpower-maintainers
